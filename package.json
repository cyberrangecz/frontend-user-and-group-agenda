{
    "name": "user-and-group-agenda",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint && npm run prettier-check",
        "e2e": "ng e2e",
        "prettier-check": "prettier --check \"projects/**/*.ts\"",
        "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
        "ci-test": "ng test --browsers ChromeHeadless --watch=false",
        "build-example-app": "ng build user-and-group-management-example --configuration production",
        "build-library": "ng build user-and-group-agenda --configuration production",
        "create-package": "cd dist/user-and-group-agenda && npm pack",
        "build-and-pack": "npm install && npm run build-library && npm run create-package",
        "bundle-report": "webpack-bundle-analyzer dist/user-and-group-management-example/stats.json",
        "ci-update-version": "cd projects/user-and-group-agenda/ && npm version $TAG_VERSION",
        "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
        "ci-publish-package": "cd dist/user-and-group-agenda && npm publish",
        "prepare": "husky"
    },
    "dependencies": {
        "@angular/animations": "^18.2.13",
        "@angular/cdk": "^18.2.14",
        "@angular/common": "^18.2.13",
        "@angular/compiler": "^18.2.13",
        "@angular/core": "^18.2.13",
        "@angular/forms": "^18.2.13",
        "@angular/material": "^18.2.13",
        "@angular/platform-browser": "^18.2.13",
        "@angular/platform-browser-dynamic": "^18.2.13",
        "@angular/router": "^18.2.13",
        "@cyberrangecz-platform/user-and-group-api": "0.0.1",
        "@cyberrangecz-platform/user-and-group-model": "0.0.1",
        "@sentinel/auth": "^18.0.3",
        "@sentinel/common": "^18.0.4",
        "@sentinel/components": "^18.2.7",
        "@sentinel/layout": "^18.1.4",
        "angular-file": "^4.0.2",
        "angular-oauth2-oidc": "^17.0.2",
        "angular-oauth2-oidc-jwks": "^17.0.2",
        "core-js": "^3.40.0",
        "file-saver": "^2.0.5",
        "ngx-indexed-db": "^18.0.0",
        "rxjs": "^7.8.1",
        "tslib": "^2.7.0",
        "typescript-collections": "^1.3.3",
        "zone.js": "^0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.14",
        "@angular-eslint/builder": "^18.4.3",
        "@angular-eslint/eslint-plugin": "^18.4.3",
        "@angular-eslint/eslint-plugin-template": "^18.4.3",
        "@angular-eslint/schematics": "^18.4.3",
        "@angular-eslint/template-parser": "^18.4.3",
        "@angular/cli": "^18.2.14",
        "@angular/compiler-cli": "^18.2.13",
        "@angular/language-service": "^18.2.13",
        "@nrwl/cypress": "^18.3.5",
        "@nrwl/workspace": "^18.3.5",
        "@types/jasmine": "^5.1.4",
        "@types/jasminewd2": "^2.0.13",
        "@types/node": "^22.6.1",
        "@typescript-eslint/eslint-plugin": "^8.24.0",
        "@typescript-eslint/parser": "^8.24.0",
        "cypress": "^10.3.0",
        "eslint": "^9.20.0",
        "husky": "^9.1.7",
        "jasmine-core": "^5.3.0",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.4.4",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage-istanbul-reporter": "^3.0.3",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "ng-packagr": "^18.2.1",
        "prettier": "^3.5.0",
        "ts-node": "^10.9.2",
        "typescript": "5.5.4",
        "webpack-bundle-analyzer": "^4.10.2"
    }
