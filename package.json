{
  "name": "@muni-kypo-crp/user-and-group-agenda",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "ci-test": "ng test --browsers ChromeHeadless --watch=false",
    "build-example-app": "ng build user-and-group-management-example --configuration production",
    "build-library": "ng build kypo-user-and-group-agenda --configuration production",
    "create-package": "cd dist/kypo-user-and-group-agenda && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "bundle-report": "webpack-bundle-analyzer dist/user-and-group-management-example/stats.json",
    "ci-update-version": "cd ./projects/kypo-user-and-group-agenda/ && npm version $VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo-user-and-group-agenda && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-check"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.3",
    "@angular/cdk": "12.0.3",
    "@angular/common": "12.0.3",
    "@angular/compiler": "12.0.3",
    "@angular/core": "12.0.3",
    "@angular/forms": "12.0.3",
    "@angular/material": "12.0.3",
    "@angular/platform-browser": "12.0.3",
    "@angular/platform-browser-dynamic": "12.0.3",
    "@angular/router": "12.0.3",
    "@sentinel/auth": "^12.0.0",
    "@sentinel/common": "~12.0.1",
    "@sentinel/components": "~12.1.1",
    "@sentinel/layout": "~12.1.3",
    "angular-oauth2-oidc": "^10.0.3",
    "angular-oauth2-oidc-jwks": "^9.0.0",
    "core-js": "^2.5.4",
    "@muni-kypo-crp/user-and-group-api": "~12.0.0",
    "@muni-kypo-crp/user-and-group-model": "~12.0.1",
    "ngx-indexed-db": "^6.1.2",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "typescript-collections": "~1.3.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1200.3",
    "@angular-devkit/build-angular": "~12.0.3",
    "@angular/cli": "~12.0.3",
    "@angular/compiler-cli": "~12.0.3",
    "@angular/language-service": "~12.0.3",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "^12.0.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "eslint": "^7.13.0",
    "@nrwl/cypress": "~12.3.3",
    "@nrwl/workspace": "~12.3.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "cypress": "^4.7.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.3",
    "prettier": "2.0.4",
