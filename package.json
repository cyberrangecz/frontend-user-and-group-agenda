    "@sentinel/common": "^18.0.2",
    "@sentinel/components": "^18.2.2",
    "@sentinel/layout": "^18.1.1",
    "angular-file": "^4.0.2",
    "angular-oauth2-oidc": "^17.0.2",
    "angular-oauth2-oidc-jwks": "^17.0.2",
    "core-js": "^3.38.1",
    "file-saver": "^2.0.5",
    "ngx-indexed-db": "^18.0.0",
    "rxjs": "^7.8.1",
