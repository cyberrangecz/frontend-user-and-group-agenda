    "@angular/platform-browser-dynamic": "^18.2.7",
    "@angular/router": "^18.2.7",
    "@muni-kypo-crp/user-and-group-api": "file:uag-api.tgz",
    "@muni-kypo-crp/user-and-group-model": "file:uag-model.tgz",
