  "compilerOptions": {
    "baseUrl": "./projects/kypo-user-and-group-agenda",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2018",
    "paths": {
      "@muni-kypo-crp/user-and-group-agenda": [
