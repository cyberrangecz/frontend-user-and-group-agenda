{
  "name": "@muni-kypo-crp/user-and-group-agenda",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "ci-test": "ng test --browsers ChromeHeadless --watch=false",
    "build-example-app": "ng build user-and-group-management-example --configuration production",
    "build-library": "ng build kypo-user-and-group-agenda --configuration production",
    "create-package": "cd dist/kypo-user-and-group-agenda && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "bundle-report": "webpack-bundle-analyzer dist/user-and-group-management-example/stats.json",
    "ci-update-version": "cd ./projects/kypo-user-and-group-agenda/ && npm version $TAG_VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo-user-and-group-agenda && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-check"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.2",
    "@angular/cdk": "13.2.2",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/forms": "13.2.2",
    "@angular/material": "13.2.2",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/router": "13.2.2",
    "@muni-kypo-crp/user-and-group-api": "~13.0.1",
    "@muni-kypo-crp/user-and-group-model": "~13.0.0",
    "@sentinel/auth": "^13.0.0",
    "@sentinel/common": "~13.0.1",
    "@sentinel/components": "~13.1.0",
    "@sentinel/layout": "~13.0.2",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-oauth2-oidc-jwks": "^13.0.1",
    "core-js": "^3.21.0",
    "file-saver": "^2.0.5",
    "ngx-indexed-db": "^10.0.0",
    "rxjs": "~7.5.4",
    "tslib": "^2.3.1",
    "typescript-collections": "~1.3.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1302.3",
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-eslint/builder": "^13.1.0",
    "@angular-eslint/eslint-plugin": "^13.1.0",
    "@angular-eslint/eslint-plugin-template": "^13.1.0",
    "@angular-eslint/schematics": "^13.1.0",
    "@angular-eslint/template-parser": "^13.1.0",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.2",
    "@angular/language-service": "~13.2.2",
    "@nrwl/cypress": "~13.8.1",
    "@nrwl/workspace": "~13.8.1",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cypress": "^9.4.1",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.2.1",
    "prettier": "2.5.1",
