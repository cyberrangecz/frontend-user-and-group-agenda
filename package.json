{
  "name": "user-and-group-management-example",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "build-lib": "ng build kypo-user-and-group-agenda --prod",
    "create-package": "cd dist/kypo-user-and-group-agenda && npm pack",
    "build-and-pack": "npm install && npm run build-lib && npm run create-package",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-lib && npm run create-package",
    "ci-publish-package": "cd dist/kypo-user-and-group-agenda && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint"
    }
  },
  "private": true,
